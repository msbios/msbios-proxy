{
  "name": "msbios/proxy",
  "description": "MSBios Proxy",
  "license": "GPL",
  "keywords": [
    "php"
  ],
  "homepage": "https://github.com/msbios/msbios-proxy",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Judzhin Miles",
      "email": "info@msbios.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "MSBios\\Proxy\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MSBiosTest\\Proxy\\": "tests/"
    }
  },
  "require": {
    "zendframework/zend-diactoros": "^1.7",
    "guzzlehttp/guzzle": "^6.3",
    "relay/relay": "^1.1",
    "msbios/msbios": "^1.0"
  },
  "require-dev": {
    "msbios/application": "^1.0",
    "msbios/test": "^1.0",
    "msbios/debug": "^1.0"
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": [
      "@cs-check",
      "phpunit --colors=always"
    ],
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
    "upload-coverage": "coveralls -v",
    "serve": "php -S 0.0.0.0:3107 -t public index.php",
    "proxy": "php -S 0.0.0.0:1986 public/proxy.php",
    "example": "php -S 0.0.0.0:1984 example.php",
    "commit": "git add . && git commit -a -m \"Lazy Composer Commiter\" && git push"
  },
  "extra": {
    "zf": {
      "component": "MSBios\\Proxy"
    }
  }
}
